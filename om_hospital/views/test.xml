<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_test_tree_view" model="ir.ui.view">
        <field name="name">action.tree.view</field>
        <field name="model">test.model</field>
        <field name="arch" type="xml">
        <tree name="Patient">
            <field name="test1_id"/>
            <field name="test2_ids"/> 
            <field name="test3_ids"/>
        </tree>
        </field>
    </record>

    <record id="action_test_view" model="ir.actions.act_window">
        <field name="name">Test</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">test.model</field>
        <field name="view_mode">tree,form,kanban,search</field>
        <field name="help" type="html">
        <p class="o_view_nocontent_smiling_face">
        Test Page</p>
        </field>
    </record>

    <menuitem
        id="menu_test_parent"
        name="Test Model"
        parent="menu_hospital_managment_system_root"
        action="action_test_view"
        sequence="10"/>

</odoo>